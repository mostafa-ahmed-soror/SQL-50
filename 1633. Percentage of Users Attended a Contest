SELECT 
    contest_id, 
    ROUND(COUNT(DISTINCT user_id) * 100.0 / (SELECT COUNT(*) FROM Users), 2) AS percentage
FROM  
    Register
GROUP BY 
    contest_id
ORDER BY 
    percentage DESC, contest_id;








// Alternative solution using joins 
SELECT r.contest_id,ROUND(COUNT(r.user_id)*100.0/(SELECT COUNT(*) FROM users) ,2) AS percentage FROM users u 
JOIN register r ON u.user_id=r.user_id
GROUP BY r.contest_id
ORDER BY percentage  DESC , r.contest_id ASC;
